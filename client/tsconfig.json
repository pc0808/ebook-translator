{
  "compilerOptions": {
    "noImplicitAny": true,
    "lib": ["es6", "dom"],
    "jsx": "react",
    "types": [
        "react",
        "react-dom"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "../",
    "baseUrl": "./",
    "outDir": "../dist/client"
  },
  "include": ["./*.ts", "./**/*.ts", "./*.tsx", "./**/*.tsx"], 
}